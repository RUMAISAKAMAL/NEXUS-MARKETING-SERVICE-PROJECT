
@{
    ViewData["Title"] = "Contact";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<main>

    <!-- breadcrumb-area-start -->
    <div class="breadcrumb-area bg-default pt-110 pb-120 has-breadcrumb-overlay" data-background="assets/img/bg/breadcrumb-img-1.png">
        <div class="container">
            <div class="row">
                <div class="col-xxl-12">
                    <div class="subnet-breadcrumb-wrap text-center">
                        <h2 class="subnet-breadcrumb-title wow" data-splitting="">Contact Us</h2>
                        <div class="subnet-breadcrumb-list wow fadeInUp" data-wow-delay=".3s">
                            <a href="index.html">Home</a>
                            <span>Contact Us</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- breadcrumb-area-end -->
    <!-- contact-page-start -->
    <div class="subnet-contact-page-area pt-140 pb-110">
        <div class="container">
            <div class="row">

                <!-- item -->
                <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6">
                    <div class="subnet-contact-page-card-item mb-30 wow fadeIn">
                        <div class="icon">
                            <i class="fas fa-map"></i>
                        </div>
                        <div class="content">
                            <h6>Office Address</h6>
                            <p>Madison Ave, New York, NY 10547</p>
                        </div>
                    </div>
                </div>

                <!-- item -->
                <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6">
                    <div class="subnet-contact-page-card-item mb-30 wow fadeIn" data-wow-delay=".3s">
                        <div class="icon">
                            <i class="fal fa-envelope"></i>
                        </div>
                        <div class="content">
                            <h6>Contact Us</h6>
                            <p>Example@gmail.com</p>
                        </div>
                    </div>
                </div>

                <!-- item -->
                <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6">
                    <div class="subnet-contact-page-card-item mb-30 wow fadeIn" data-wow-delay=".4s">
                        <div class="icon">
                            <i class="fal fa-phone-alt"></i>
                        </div>
                        <div class="content">
                            <h6>Telephone</h6>
                            <p>(88)123 657 6548 3247</p>
                        </div>
                    </div>
                </div>

                <!-- title -->
                <div class="col-12">
                    <div class="subnet-contact-page-title-wrap text-center mt-110 mb-60">
                        <h6 class="subnet-subtitle-2 wow fadeIn" data-wow-delay=".3s">
